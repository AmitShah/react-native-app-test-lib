{
  "name": "go-network-simulator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "mkdir -p ./build && npm run vis-compile && npm run app-compile && npm run server-compile",
    "rn": "node_modules/.bin/react-native start",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "node_modules/tslint/bin/tslint -c tslint.json 'src/**/*.ts'",
    "vis-clear": "(rm -rf build/vis || true) && mkdir build/vis",
    "vis-compile": "node scripts/vis-compile",
    "app-clear": "rm -rf build/app || true",
    "app-compile": "npm run app-clear && node_modules/.bin/tsc -b ./src/app",
    "app-watch": "npm run app-clear && node_modules/.bin/tsc -b ./src/app --watch --verbose",
    "server-clear": "rm -rf build/server || true",
    "server-compile": "npm run server-clear && node_modules/.bin/tsc -b ./src/server/tsconfig.json",
    "server": "npm run server-compile && node build/server"
  },
  "dependencies": {
    "assert": "^1.4.1",
    "buffer": "^5.1.0",
    "d3": "^5.7.0",
    "ethereumjs-util": "^5.2.0",
    "ethereumjs-wallet": "^0.6.2",
    "ganache-core": "https://github.com/unao/ganache-core.git#43ac98ac5a58c9e0d1008ec7b226a1e6bfaa5111",
    "go-network-framework": "^0.2.33",
    "mqtt-connection": "^4.0.0",
    "native-base": "^2.8.1",
    "node-libs-browser": "^2.1.0",
    "process": "^0.11.10",
    "qrcode": "^1.2.2",
    "react": "^16.5.0",
    "react-native": "^0.57.1",
    "react-native-camera": "^1.1.4",
    "react-native-keychain": "^3.0.0-rc.3",
    "react-native-paho-mqtt": "*",
    "react-native-securerandom": "^0.1.1",
    "react-native-vector-icons": "^4.6.0",
    "rxjs": "^5.5.11",
    "websocket-stream": "^5.1.2",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@types/d3": "^5.0.0",
    "@types/node": "^10.7.1",
    "@types/qrcode": "^1.2.0",
    "@types/react": "^16.3.1",
    "@types/react-native": "^0.56.24",
    "@types/ws": "^6.0.0",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "^5.0.0",
    "jest": "22.4.3",
    "memdown": "^3.0.0",
    "react-test-renderer": "16.3.1",
    "schedule": "^0.4.0",
    "truffle-contract": "^3.0.6",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "types-ethereumjs-util": "0.0.8",
    "typescript": "^3.1.3",
    "web3": "^1.0.0-beta.35"
  },
  "jest": {
    "preset": "react-native"
  }
}
